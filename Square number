// Add some code
#include<stdio.h>
#include<math.h>


int main()
{
  int T,i,N,result,max;

  scanf("%d",&T);

  for(i=1;i<=T;i++)
  {
      scanf("%d",&N);
      result=sqrt(N);

      max=result*result;

      if(max == N)
      {
          printf("YES\n");
      }
      else
      {
          printf("NO\n");
      }

  }
  return 0;
}
